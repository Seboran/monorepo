{
  "name": "generateur-musique",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "build": "vite build",
    "preview": "pnpm -C ./packages/app-visualisation run preview",
    "dev": "vite dev",
    "format": "prettier --write --cache .",
    "format:check": "prettier --check .",
    "coverage": "vitest run --coverage",
    "coverage:preview": "npx vite preview --outDir ./coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "lib-musique": "workspace:*",
    "lib-style": "workspace:*",
    "lib-sudoku": "workspace:*",
    "opensheetmusicdisplay": "^1.8.4",
    "vue": "^3.4.3"
  },
  "devDependencies": {
    "@tsconfig/node18": "18.2.4",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.10.10",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "autoprefixer": "10.4.20",
    "happy-dom": "16.3.0",
    "npm-run-all2": "7.0.2",
    "postcss": "8.4.49",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3",
    "vite": "6.0.10",
    "vitest": "2.1.8",
    "vue-tsc": "2.2.0"
  }
}
