{
  "$schema": "https://codemod-utils.s3.us-west-1.amazonaws.com/configuration_schema.json",
  "name": "vue-2-to-vue-3-refs-and-methods",
  "version": "0.0.3",
  "engine": "jscodeshift",
  "private": false,
  "include": ["**/*.js", "**/*.jsx", "**/*.ts", "**/*.tsx", "**/*.vue"],
  "applicability": {
    "from": [
      ["vue", ">=", "2.0.0"],
      ["vue", "<", "3.0.0"]
    ],
    "to": [["vue", ">=", "3.0.0"]]
  },
  "arguments": [],
  "meta": {
    "tags": ["vue", "3", "migration"],
    "git": "https://github.com/Seboran/monorepo/tree/main/packages/nirina-codemods"
  }
}
